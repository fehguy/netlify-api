{
    "name": "netlify-api",
    "version": "1.0.0",
    "description": "Simple API",
    "repository": "fehguy/netlify-api",
    "main": "index.js",
    "scripts": {
        "build": "npm run build:lambda:deps && npm run build:lambda:bundle && npm run build:lambda:build",
        "build:lambda:deps": "npm i -g netlify-cli",
        "build:lambda:bundle": "cd myFunctions/src && npm i",
        "build:lambda:build": "netlify functions:build --src myFunctions/src"
    },
